<html>
	<head>
		<script src="jquery-2.1.1.min.js"></script>
		<script>
			function doSomething(){
				$("input:button").after("<input type='text'>");
			}
			$().ready(function(){
				$("input:button").on("click",doSomething);
			});
		</script>
	</head>	
	<body>
		<input type="button" value="click">
	</body>
</html>	